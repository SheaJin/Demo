<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.qingxu.demoapp">

    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".ui.recycler.MainActivity" />
        <activity android:name=".ui.main.EmptyActivity"/>
        <activity android:name="com.qingxu.demoapp.ui.databinding.DataBindingActivity" />
        <activity android:name="com.qingxu.demoapp.ui.databinding.DataFragmentActivity" />
        <activity android:name=".ui.rank.RankActivity"/>
        <activity android:name=".ui.loadmore.TextLoadMoreActivity"></activity>
        <activity android:name=".ui.login.RegisterActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <meta-data
            android:name="design_width"
            android:value="1242" />
        <meta-data
            android:name="design_height"
            android:value="2208" />
    </application>
</manifest>