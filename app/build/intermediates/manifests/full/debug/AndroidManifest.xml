<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.qingxu.demoapp"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="21"
        android:targetSdkVersion="27" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:debuggable="true"
        android:icon="@mipmap/logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:testOnly="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar" >
        <activity android:name="com.qingxu.demoapp.ui.recycler.MainActivity" />
        <activity android:name="com.qingxu.demoapp.ui.main.EmptyActivity" />
        <activity android:name="com.qingxu.demoapp.ui.databinding.DataBindingActivity" />
        <activity android:name="com.qingxu.demoapp.ui.databinding.DataFragmentActivity" />
        <activity android:name="com.qingxu.demoapp.ui.rank.RankActivity" />
        <activity android:name="com.qingxu.demoapp.ui.loadmore.TextLoadMoreActivity" />
        <activity android:name="com.qingxu.demoapp.ui.login.RegisterActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="design_width"
            android:value="1242" />
        <meta-data
            android:name="design_height"
            android:value="2208" />
    </application>

</manifest>